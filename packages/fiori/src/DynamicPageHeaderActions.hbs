<div class="{{classes.root}}">
    <div class="{{classes.wrapper}}">
        <ui5-button
            class="{{classes.expandCollapseButton}}"
            @click="{{onExpandClick}}" 
            icon="{{arrowButtonIcon}}"
            accessible-name="{{expandLabel}}"
            .accessibilityAttributes="{{accessibilityAttributes}}"
            title="{{expandLabel}}"
        ></ui5-button>
    {{#unless snapped}}
        <ui5-toggle-button
            class="{{classes.pinButton}}"
            @click="{{onPinClick}}"
            icon="{{pinButtonIcon}}"
            ?pressed="{{pinned}}"
            .accessibilityAttributes="{{accessibilityAttributes}}"
            accessible-name="{{pinLabel}}"
            title="{{pinLabel}}"
        ></ui5-toggle-button>
    {{/unless}}
    </div>
</div>
