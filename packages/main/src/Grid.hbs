<div id="grid" role="grid"
    style="{{styles.grid}}"
    aria-label="{{_ariaLabel}}"
    aria-multiselectable="{{_ariaMultiSelectable}}"
>
    <slot name="header-row"></slot>
    <slot></slot>
    {{#unless rows.length}}
        <ui5-grid-row tabindex="0">
            <ui5-grid-cell id="nodata-cell">
            {{#if nodata.length}}
                <slot name="nodata"></slot>
            {{else}}
                {{_effectiveNoDataText}}
            {{/if}}
            </ui5-grid-cell>
        </ui5-grid-row>
    {{/unless}}
</div>
