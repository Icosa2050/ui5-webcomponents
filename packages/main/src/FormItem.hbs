<div class="ui5-form-item-root">
	<div class="ui5-form-item-label">
		{{#if hasLabelContent}}
			<slot name="labelContent"></slot>
		{{else}}
			<ui5-label>{{ label }}:</ui5-label>
		{{/if}}
	</div>
	<div class="ui5-form-item-content">
		{{#each items}}
		<div class="ui5-form-item-content-child">
			<slot name="{{this._individualSlot}}"></slot>
		</div>
		{{/each}}
	</div>
</div>