{{#if _showMoreColors}}
<ui5-dialog
    header-text="{{_moreColorsFeatureDialogTitle}}"
>
    <div class="ui5-cp-dialog-content">
        <ui5-color-picker></ui5-color-picker>
    </div>

    <div slot="footer" class="ui5-cp-dialog-footer">
        <ui5-button
            design="Emphasized"
            @click="{{_chooseCustomColor}}"
        >{{_moreColorsFeatureDialogOKButton}}</ui5-button>
        <ui5-button
            design="Transparent"
            @click="{{_closeDialog}}"
        >{{_moreColorsFeatureDialogCancelButton}}</ui5-button>
    </div>
</ui5-dialog>
{{/if}}
