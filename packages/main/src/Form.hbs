<div class="ui5-form-root" role="form" aria-labelledby="_ariaLabelledBy" ?editable="{{editable}}">
	<div class="ui5-form-header" part="header">
		{{#if hasCustomHeader}}
			<slot name="header"></slot>
		{{else}}
			<ui5-title id="{{_id}}-header-text" level="H4">{{ headerText }}</ui5-title>
		{{/if}}
	</div>

	<div class="ui5-form-layout" part="layout">
		{{#each items}}
			<div class="ui5-form-layout-item" style="grid-column: span 2">
				<slot name="{{this._individualSlot}}"></slot>
			</div>
		{{/each}}
	</div>
</div>