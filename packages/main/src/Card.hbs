<div
	class="{{classes.root}}"
	role="region"
	aria-label="{{_getAriaLabel}}">
	{{#if _hasHeader}}
		<div class="ui5-card-header-root">
			<slot name="header"></slot>
		</div>
	{{/if}}
	<div role="group" aria-label="{{_ariaCardContentLabel}}" class="{{classes.content}}">
		<slot></slot>
	</div>
</div>
