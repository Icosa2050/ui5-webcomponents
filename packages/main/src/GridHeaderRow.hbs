{{#if _hasSelectionComponent}}
    <ui5-grid-header-cell id="selection-cell"
        aria-selected="{{_selected}}"
        aria-label="{{_i18nSelection}}"
        @keydown="{{_onSelectionCellKeyDown}}"
    >
    {{#if _isMultiSelect}}
        <ui5-checkbox id="selection-component" tabindex="-1"
            ?checked="{{_selected}}"
            @ui5-change="{{_onSelectAllChange}}"
            accessible-name="{{_i18nRowSelector}}"
        ></ui5-checkbox>
    {{/if}}
    </ui5-grid-header-cell>
{{/if}}

{{#each _visibleCells}}
	<slot name="{{_individualSlot}}"></slot>
{{/each}}

{{#if _popinCells.length}}
    <ui5-grid-header-cell id="popin-cell" aria-label="{{_i18nRowPopin}}"></ui5-grid-cell>
{{/if}}
